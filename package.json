{
  "name": "CloudWorkerApp",
  "productName": "CloudWorkerApp",
  "author": "CloudFactory",
  "version": "1.1.1",
  "description": "CloudWorker App",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "repository": "https://github.com/asesh/electron-browser",
  "keywords": [
    "Cloud",
    "Worker",
    "Hub"
  ],
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^18.3.7",
    "electron-builder": "^20.36.2",
    "electron-winstaller": "^2.7.0",
    "electron-log": "^1.3.0",
    "electron-updater": "^1.4.2",
    "http-server": "^0.9.0"
  },
  "build": {
    "asar": true,
    "appId": "com.cloudfactory.cloudworker",
    "mac": {
      "category": "your.app.category.type"
    },
    "nsis": {
        "oneClick": true,
        "perMachine": true,
        "allowElevation": true,
        "menuCategory": true
    },
    "publish": {
        "provider": "generic",
        "url": "http://127.0.0.1:8080/"
    }
  },
  "dependencies": {
    "@types/nprogress": "0.0.29",
