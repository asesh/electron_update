    "autoprefixer": "7.1.6",
    "axios": "^0.18.0",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-app": "^6.1.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "concurrently": "^4.0.1",
    "css-loader": "1.0.0",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "electron-is-dev": "^1.0.1",
    "electron-log": "^2.2.17",
    "electron-updater": "^4.0.4",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
